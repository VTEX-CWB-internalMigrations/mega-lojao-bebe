{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "footer-spacer",
      "flex-layout.col#footerOneRowZero",
      "flex-layout.col#footerOneRowOne",
      "flex-layout.col#footerOneRowTwo",
      "footer-spacer"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.col#footerOneRowZero":{
    "children":[
      "rich-text#institutionalTitle",
      "vtex.menu@2.x:menu#footerfirst"
    ]
  },
  "rich-text#institutionalTitle":{
    "props":{
      "text": "**Institucionais**"
    }
  },
  "flex-layout.col#footerOneRowOne": {
    "children": [
      "rich-text#payments",
      "image#payment"
    ],
    "props":{
      "blockClass": "payment-footer-col"
    }
  },
  "flex-layout.col#footerOneRowTwo": {
    "children": [
      "rich-text#atendimento",
      "rich-text#iframe"
    ]
  },
  "rich-text#payments": {
    "props": {
      "text": "**Métodos de Pagamentos**",
      "blockClass": "footerText"
    }
  },
  "image#payment":{
    "props":{
      "src": "",
      "blockClass": "payment"
    }
  },
  "rich-text#atendimento": {
    "props": {
      "text": "**Atendimento**",
      "blockClass": "footerText"
    }
  },
  "rich-text#iframe":{
    "props":{
      "text": "Iframe Here",
      "blockClass": "iframe-box"
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "children": [
      "footer-spacer",
      "rich-text#contact",
      "social-networks",
      "footer-spacer"
    ],
    "props": {
      "blockClass": "social-footer"
    }
  },
  "rich-text#contact":{
    "props":{
      "text": "Central de Atendimento: (41) 99238 0533 <br /> Segunda á Sexta das 09:00 as 18:00hs.",
      "blockClass": "contact-footer"
    }
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": [
        "MasterCard",
        "Visa",
        "Diners Club"
      ]
    }
  },
  "flex-layout.row#footer-3-desktop": {
    "children": [
      "footer-spacer",
      "rich-text#footer",
      "rich-text#vtexImage",
      "footer-spacer"
    ],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-mobile",
      "rich-text#vtex-image-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": [
      "flex-layout.col#footer-1-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks",
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": [
      "vtex.menu@2.x:menu#footer-mobile"
    ]
  },
  "flex-layout.row#footer-3-mobile": {
    "children": [
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "RUA PROF.BRASÍLIO O. DA COSTA, 1580 - STA.QUITÉRIA CURITIBA - PR CNPJ. 10993915/0001-07",
      "blockClass": "mobilefooter"
    }
  },
  "rich-text#vtexImage":{
    "props":{
      "text": "VTEX Logo Here",
      "blockClass": "vtex-logo"
    }
  },
  "rich-text#vtex-image-mobile":{
    "props":{
      "text": "![Imagem do logotipo da VTEX](https://megalojaodobebe.vteximg.com.br/arquivos/vtexLogoMain.png)",
      "blockClass": "vtex-logo"
    }
  }
}